import tkinter as tk
from tkinter import messagebox

class ScheduleGUI:

    def add_buttons(self):
        self.add_button = tk.Button(self.frame, text="Add Task")
        self.remove_button = tk.Button(self.frame, text="Remove Task")
        self.edit_button = tk.Button(self.frame, text="Edit Task")
        self.view_button = tk.Button(self.frame, text="View Task")
        self.quit_button = tk.Button(self.frame, text="Quit")

    def __init__(self, master=None):
        tk.Tk.__init__(self, master)
        self.master = master
        self.master.title("Schedule Manager")
        self.master.geometry("800x600")
        self.master.resizable(False, False)
        self.create_widgets()
        self.add_buttons()

app = ScheduleGUI()
app.mainloop()
